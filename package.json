{
  "name": "ngx-supabase-auth",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start:demo-app\" \"npm run start:demo-app-electron\" \"npm run start:demo-server\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib": "ng build ngx-supabase-auth --project ngx-supabase-auth && npm run copy:readme",
    "start:demo-app": "ng serve demo-app",
    "start:demo-app-electron": "concurrently \"ng serve demo-app-electron\" \"wait-on http://localhost:4300 && node projects/demo-app-electron/electron/electron.js\"",
    "start:demo-server": "node projects/demo-server/main.js",
    "copy:readme": "cp README.md dist/ngx-supabase-auth/"
  },
  "private": false,
  "dependencies": {
    "@angular-architects/ngrx-toolkit": "^19.1.0",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@ngrx/signals": "^19.0.0",
    "@supabase/supabase-js": "^2.39.7",
    "@tailwindcss/postcss": "^4.0.12",
    "daisyui": "^5.0.0",
    "prettier": "^3.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.12",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@electron/remote": "^2.1.2",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^22.13.10",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.7",
    "dotenv-webpack": "^8.1.0",
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "postcss": "^8.5.3",
    "typescript": "~5.5.0",
    "wait-on": "^8.0.3"
  }
}
